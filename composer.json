{
    "name": "ed.sukharev/request-headers-param-converter",
    "description": "This param converter for Sensio Framework Extra Bundle adds convenient HTTP headers handling",
    "keywords": ["symfony","paramconverter","param converter","api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eduard Sukharev",
            "email": "sukharev.eh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "sensio/framework-extra-bundle": ">=2.0.0"
    },
    "config": {
        "platform": {
            "php": "5.4.45"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "EdSukharev\\App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdSukharev\\App\\Test\\": "tests/"
        }
    }
}
